{
    "name": "comunidade-catolica-agape",
    "version": "1.0.0",
    "description": "site criado com intuito de ajudar a comunidade católica ágape em sua missão de fé.",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/src/main/presentation/http/app.express.js",
        "dev-start": "node --import=tsx --watch src/main/presentation/http/app.express.ts",
        "dev:server": "nodemon",
        "dev:console": "node -r tsconfig-paths/register -r sucrase/register src/index.ts",
        "test": "vitest",
        "test:run": "vitest run",
        "test:ui": "vitest --ui",
        "test:verbose": "vitest --reporter verbose"
    },
    "prisma": {
        "schema": "./src/main/infra/database/orm/prisma/schema.prisma"
    },
    "keywords": [
        "comunidade",
        "comunidade",
        "católica",
        "ágape",
        "comunidade",
