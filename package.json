        "fé",
        "grupo",
        "católico",
        "programação",
        "igreja",
        "santos"
    ],
    "author": "David Pontes de Jesus, Adson Vicente Santos Menezes e Vitor Nascimento de Souza",
    "license": "ISC",
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@types/bcrypt": "^5.0.2",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/date-fns": "^2.5.3",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.12",
        "@types/node": "^20.12.4",
        "@types/nodemailer": "^6.4.15",
        "@types/supertest": "^6.0.2",
        "@types/winston": "^2.4.4",
        "@vitest/ui": "^1.5.0",
        "bcrypt": "^5.1.1",
        "nodemon": "^3.1.0",
        "sucrase": "^3.35.0",
        "supertest": "^6.3.4",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.4.4",
        "vitest": "^1.4.0",
        "vitest-mock-extended": "^1.3.1"
    },
